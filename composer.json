{
    "name": "analyzen/auth",
    "description": "This is an auth package",
    "type": "laravel-package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Analyzen\\Auth\\": "src/",
            "Analyzen\\Auth\\Database\\Factories\\": "database/factories/",
            "Analyzen\\Auth\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "sakhawathossensadi",
            "email": "newtonss919@gmail.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Analyzen\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel" : {
            "providers": [
                "Analyzen\\Auth\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev",
        "orchestra/testbench": "^7.0"
    },
    "minimum-stability": "dev",
    "require": {
        "laravel/passport": "^10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter",
        "test-w": "vendor\\bin\\phpunit",
        "test-wf": "vendor\\bin\\phpunit --filter"
    }
}
